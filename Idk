1-- Create the GUI
2local gui = Instance.new("ScreenGui")
3gui.Name = "ShockHub"
4gui.Parent = game.Players.LocalPlayer.PlayerGui
5
6local mainFrame = Instance.new("Frame")
7mainFrame.Size = UDim2.new(0, 200, 0, 100)
8mainFrame.Position = UDim2.new(0, 10, 0, 10)
9mainFrame.BackgroundColor3 = Color3.new(0, 0, 0)
10mainFrame.BackgroundTransparency = 0.5
11mainFrame.Parent = gui
12
