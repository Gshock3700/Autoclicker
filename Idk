14title.Text = "ShockHub"
15title.Font = Enum.Font.SourceSans
16title.FontSize = Enum.FontSize.Size24
17title.TextColor3 = Color3.new(1, 1, 1)
18title.Parent = mainFrame
19
20-- Create the autoclicker toggle button
21local autoclickerToggle = Instance.new("TextButton")
22autoclickerToggle.Text = "Toggle Autoclicker"
23autoclickerToggle.Font = Enum.Font.SourceSans
24autoclickerToggle.FontSize = Enum.FontSize.Size18
25autoclickerToggle.TextColor3 = Color3.new(1, 1, 1)
26autoclickerToggle.BackgroundColor3 = Color3.new(0, 0, 0)
27autoclickerToggle.BackgroundTransparency = 0.5
28autoclickerToggle.Size = UDim2.new(0, 150, 0, 30)
29autoclickerToggle.Position = UDim2.new(0, 10, 0, 40)
30autoclickerToggle.Parent = mainFrame
31
32local autoclickerCircle = Instance.new("ImageLabel")
33autoclickerCircle.Image = "rbxassetid://3570695787" -- small circle image
34autoclickerCircle.Size = UDim2.new(0, 20, 0, 20)
35autoclickerCircle.Position = UDim2.new(1, -30, 0, 5)
36autoclickerCircle.Parent = autoclickerToggle
37
