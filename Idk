49
50-- Create the clicks per second textbox
51local clicksPerSecondTextBox = Instance.new("TextBox")
52clicksPerSecondTextBox.PlaceholderText = "Clicks per second"
53clicksPerSecondTextBox.Font = Enum.Font.SourceSans
54clicksPerSecondTextBox.FontSize = Enum.FontSize.Size18
55clicksPerSecondTextBox.TextColor3 = Color3.new(1, 1, 1)
56clicksPerSecondTextBox.BackgroundColor3 = Color3.new(0, 0, 0)
57clicksPerSecondTextBox.BackgroundTransparency = 0.5
58clicksPerSecondTextBox.Size = UDim2.new(0, 100, 0, 30)
59clicksPerSecondTextBox.Position = UDim2.new(0, 10, 0, 120)
60clicksPerSecondTextBox.Parent = mainFrame
61
62-- Variables
63local autoclickerEnabled = false
64local areaToClick = Vector2.new(0, 0)
65local clicksPerSecond = 1
66
67-- Functions
68local function toggleAutoclicker()
69    autoclickerEnabled = not autoclickerEnabled
70    if autoclickerEnabled then
71        autoclickerCircle.ImageColor3 = Color3.new(1, 0, 0) -- red
72        autoclickerToggle.Text = "Toggle Autoclicker (ON)"
73    else
74        autoclickerCircle.ImageColor3 = Color3.new(0, 0, 0) -- black
75        autoclickerToggle.Text = "Toggle Autoclicker (OFF)"
76    end
77end
78
79local function setAreaToClick()
80    local userInputService = game:GetService("UserInputService")
81    userInputService.InputBegan:Connect(function(input)
82        if input.UserInputType == Enum.UserInputType.Touch then
83            areaToClick = input.Position
84        end
85    end)
86end
87
88local function startAutoclicker()
89    while autoclickerEnabled do
90        wait(1 / clicksPerSecond)
91        game
